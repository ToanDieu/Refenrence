  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "analyze:build": "NODE_ENV=production webpack --mode production --progress --profile --json > stats.json",
    "preanalyze": "npm run analyze:clean && npm run analyze:build",
    "analyze": "webpack-bundle-analyzer stats.json",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production karma start --single-run",
    "test:watch": "NODE_ENV=development karma start --auto-watch",
    "test:jest": "NODE_ENV=test jest --coverage",
    "pretest:jest": "rimraf ./coverage",
    "prebuild": "npm run build:clean",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production webpack --mode production --progress --hide-modules",
    "build:clean": "rimraf ./dist",
    "build:dev": "NODE_ENV=development webpack --mode development --progress --hide-modules",
    "dev": "NODE_ENV=development webpack-dev-server --port 3000 --mode development",
    "stories": "start-storybook --ci --host 127.0.0.1 --port 8989"
  },
